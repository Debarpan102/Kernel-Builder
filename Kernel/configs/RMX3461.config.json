[
  {
    "kernelSource": {
      "name": "android_kernel_oplus_RMX3461",
      "repo": "https://github.com/crdroidandroid/android_kernel_oplus_RMX3461.git",
      "branch": "14.0",
      "device": "RMX3461",
      "defconfig": "gki_defconfig"
    },
    "toolchains": [
      {
        "name": "neutron-clang",
        "repo": "https://github.com/Debarpan102/neutron-clang.git",
        "branch": "main",
        "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "proton-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "proton-clang/bin/arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
